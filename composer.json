{
  "name": "silecust/silecust",
  "description": "Web Shop that helps you to set up online quickly",
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.3",
    "symfony/framework-bundle": "^7.2",
    "symfony/phpunit-bridge": "^7.2",
    "symfony/console": "^7.2",
    "symfony/yaml": "^7.2",
    "silecust/framework": "dev-master",
    "doctrine/dbal": "^3",
    "doctrine/doctrine-bundle": "^2.11",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "^3.0",
    "monolog/monolog": "^3.8",
    "symfony/monolog-bundle": "^3.10",
    "doctrine/migrations": "^3.8",
    "symfony/security-bundle": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "zenstruck/browser": "^1.8",
    "zenstruck/console-test": "^1.5",
    "zenstruck/foundry": "^1.37",
    "zenstruck/mailer-test": "^1.4",
    "phpunit/phpunit": "^10.4"
  }
}
