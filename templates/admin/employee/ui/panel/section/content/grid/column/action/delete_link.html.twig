{% set context_route= request.session.get('context_route') != null ?request.session.get('context_route') : 'sc_admin_panel' %}
{% set after =  path(context_route,
    {
        '_function': function,
        '_type':'list',
        'id': listGrid.id}) %}
{% set href = path(context_route,
    {
        '_function': function,
        '_type': 'delete',
        'id': id,
        '_redirect_upon_success_url':after
    }) %}
<div class="d-flex flex mb-1 ml-0 ">
    <a class="link-primary" id="{{ link_id ~"-"~id }}" href="{{ href }}">
        <i class="bi bi-trash pe-2"></i>
    </a>
</div>
