{# templates/user/create.html.twig #}
{% extends 'base/admin_base_template.html.twig' %}
{% block header %}
  {% include 'admin/ui/header/user_logged_in.html.twig' %}
{% endblock %}
 {% block sidebar %}
    {% include 'admin/ui/sidebar/nav.html.twig' with {'sections':sidebarMenu.sections} %}
 {% endblock %}
{% block content %}
    {% if app.request.get('type') == 'list' %}

        {% include "admin/ui/panel/_create_button_in_list.html.twig" %}
    {% elseif app.request.get('type') == 'display' %}

        {% include "admin/ui/panel/_display_button_in_list.html.twig" %}
    {% endif %}

    <div class="container">
        {{ content|raw }}
    </div>
{% endblock %}