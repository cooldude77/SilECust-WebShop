{% if items is not null and items|length > 0 %}
    <div class="container">
        {% for item in items %}
            <div class="row">
                <div class="col-sm-4" style="background-color:lavender;">
                    <a href="{{ path(app.session.get('context_route'),
                        {'function':app.request.get('function'),id:item.id,
                            'type':'display'}) }}">   {{ item.code }}</a>
                </div>
                <div class="col-sm-4" style="background-color:lavender;">
                    <a href="{{ path(app.session.get('context_route'),
                        {'function':'item',id:item.id,
                            'type':'edit'}) }}"> Edit</a>
                </div>
            </div>
        {% endfor %}
    </div>
{% endif %}