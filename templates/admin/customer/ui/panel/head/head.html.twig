{% block head %}
    <meta charset="UTF-8">
    {% block javascripts %}
        {% if app.environment != 'test' %}
            {% block importmap %}
                {{ importmap('app') }}
            {% endblock %}
        {% endif %}
    {% endblock %}
{% endblock %}
<title>
    {% set constant = constant('Silecust\\Framework\\Service\\Twig\\TwigConstants::UI_WEB_PAGE_TITLE') %}
    {% if (request.attributes.get(constant) !=null) %}
        {{ request.attributes.get(constant) }}
    {% else %}
        {{ 'This page doesn\'t have a title' }}
    {% endif %}
</title>
